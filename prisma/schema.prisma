// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Quote {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now())
    title     String
    text      String
}

model Command {
    id          String   @id @default(cuid())
    createdAt   DateTime @default(now())
    commandName String
    userId      String
}
